{
  "name": "@datalinkhq/datalink",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "git submodule update --init --remote",
    "update": "ncu",
    "upgrade": "ncu -u",
    "dev": "node dev",
    "dev:next": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "pnpm schema:generate && next build",
    "build:turbo": "pnpm schema:generate && next build --turbo",
    "schema:proxy-generate": "prisma generate --data-proxy",
    "schema:generate": "prisma generate",
    "schema:push": "prisma db push",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "export:turbo": "next export --turbo",
    "build:vercel": "pnpm build && ./vercel.sh",
    "build:bootstrap": "ncc build bin/bootstrapper.ts -o bin/datalink-server -d",
    "compile:bootstrap": "tsc bin/bootstrapper.ts --noEmit & pkg ./bin/bootstrapper.js -o datalink-server -d -t node18-linux-x64 && mv datalink-server bin/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@prisma/client": "^4.8.0",
    "@sentry/nextjs": "^7.17.4",
    "ansi-colors": "^4.1.3",
    "axios": "^1.3.2",
    "chart.js": "4.2.0",
    "color-support": "^1.1.3",
    "framer-motion": "^8.4.3",
    "intercept-stdout": "^0.1.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "next": "^13.0.1",
    "next-themes": "^0.2.1",
    "nookies": "^2.5.2",
    "react-chartjs-2": "^4.3.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.0",
    "uuid": "^9.0.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "@faker-js/faker": "7.6.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/download": "^8.0.2",
    "@types/glob": "8.0.0",
    "@types/intercept-stdout": "^0.1.0",
    "@types/jsonwebtoken": "8.5.9",
    "@types/lodash": "4.14.187",
    "@types/node": "18.11.9",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/uuid": "8.3.4",
    "@vercel/ncc": "^0.36.0",
    "autoprefixer": "^10.4.13",
    "download": "^8.0.0",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.1",
    "extract-zip": "^2.0.1",
    "node-stream-zip": "^1.15.0",
    "npm-check-updates": "16.3.18",
    "pkg": "^5.8.0",
    "postcss": "^8.4.21",
    "prisma": "^4.8.0",
    "react": "18.2.0",
    "stream-file": "^0.3.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typescript": "4.9.5",
    "webpack": "5.75.0"
  }
}
