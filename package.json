{
  "name": "@datalinkhq/datalink",
  "version": "0.1.0",
  "private": true,
  "scripts": {    
    "update": "ncu",
    "upgrade": "ncu -u",

    "dev": "node dev",
    "dev:next": "next dev",
    "dev:turbo": "next dev --turbo",

    "build": "pnpm schema:generate && next build",
    "build:turbo": "pnpm schema:generate && next build --turbo",
    "build:vercel": "pnpm build && ./vercel.sh",
    "build:bootstrap": "ncc build bin/bootstrapper.ts -o bin/datalink-server -d",
    "compile:bootstrap": "tsc bin/bootstrapper.ts --noEmit & pkg ./bin/bootstrapper.js -o datalink-server -d -t node18-linux-x64 && mv datalink-server bin/",

    "export": "next export",
    "export:turbo": "next export --turbo",

    "schema:proxy-generate": "prisma generate --data-proxy",
    "schema:generate": "prisma generate",
    "schema:push": "prisma db push",
    
    "start": "next start",
    "lint": "next lint",

    "preinstall": "git submodule update --init --remote"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "@sentry/nextjs": "^7.36.0",
    "ansi-colors": "^4.1.3",
    "axios": "^1.3.2",
    "chart.js": "4.2.1",
    "color-support": "^1.1.3",
    "intercept-stdout": "^0.1.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "next": "^13.1.6",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "@faker-js/faker": "7.6.0",
    "@types/download": "^8.0.2",
    "@types/glob": "8.0.1",
    "@types/intercept-stdout": "^0.1.0",
    "@types/jsonwebtoken": "9.0.1",
    "@types/lodash": "4.14.191",
    "@types/node": "18.11.19",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/uuid": "9.0.0",
    "@vercel/ncc": "^0.36.1",
    "download": "^8.0.0",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "node-stream-zip": "^1.15.0",
    "npm-check-updates": "16.6.4",
    "pkg": "^5.8.0",
    "prisma": "^4.9.0",
    "react": "18.2.0",
    "stream-file": "^0.3.0",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typescript": "4.9.5",
    "webpack": "5.75.0"
  }
}
